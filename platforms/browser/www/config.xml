<?xml version='1.0' encoding='utf-8'?>
<widget id="com.FutGirl" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FutGirl</name>
    <description>
    Aplicación para encontrar equipos contra quién jugar o también jugadores 
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    </author>
    <content src="public/login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
